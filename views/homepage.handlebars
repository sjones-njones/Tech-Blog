<section class="blog container">
  <div class='row'>

  {{#each blog as |blog|}}
  <div class='blog bg-light col-md-3'>
    <p>
      {{blog.title}},
      <br>
      {{blog.post}}
    </p>
  </div>
  <div class='col-md-9'>
    <div class = 'row'>
<div class="col-8 col-md-6">

    <p>
      User:
      {{user.userName}}
    </p>
  </div>
  {{/each}}
</div>
    </div>
  </div>
</section>